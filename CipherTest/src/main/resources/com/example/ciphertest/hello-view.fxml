<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="673.0" prefWidth="732.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ciphertest.HelloController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="649.0" prefWidth="699.0" VBox.vgrow="ALWAYS">
            <children>
                <TextArea fx:id="filePath" layoutX="14.0" layoutY="14.0" prefHeight="94.0" prefWidth="587.0" promptText="Please, enter polynomials and sync bit" wrapText="true" />
                <TextArea fx:id="originText" layoutX="14.0" layoutY="135.0" prefHeight="222.0" prefWidth="587.0" promptText="Original message" wrapText="true" />
                <TextArea fx:id="cryptoText" layoutX="14.0" layoutY="443.0" prefHeight="214.0" prefWidth="587.0" promptText="Crypted message" wrapText="true" />
                <TextField fx:id="initStateField" layoutX="14.0" layoutY="398.0" prefHeight="26.0" prefWidth="587.0" promptText="Initial state of the key" />
                <Button layoutX="608.0" layoutY="48.0" mnemonicParsing="false" onAction="#load" prefHeight="25.0" prefWidth="115.0" text="Clear" />
                <Button layoutX="632.0" layoutY="233.0" mnemonicParsing="false" onAction="#encrypt" prefHeight="25.0" prefWidth="68.0" text="Encrypt" />
                <Button layoutX="632.0" layoutY="281.0" mnemonicParsing="false" onAction="#decrypt" prefHeight="25.0" prefWidth="68.0" text="Decrypt" />
                <Button layoutX="625.0" layoutY="398.0" mnemonicParsing="false" onAction="#autoFillOfInit" prefHeight="25.0" prefWidth="81.0" text="Auto state" />
                <Button layoutX="625.0" layoutY="523.0" mnemonicParsing="false" onAction="#loadTests" prefHeight="26.0" prefWidth="82.0" text="Tests" />
            </children>
        </AnchorPane>
    </children>
</VBox>
